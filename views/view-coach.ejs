<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>

<body>
    <%- include('includes/navigation.ejs') %>

    <main>
        <% if (coachh.length > 0) { %>
            <div class="grid">
                <% for (let coaches of coachh) { %>
                    <article class="card product-item">
                        <header class="card__header">
                            <h1 class="product__title"><%= coachh.name %></h1>
                        </header>
                        <div class="card__image">
                            <img src="data:image/jpeg;base64," alt="A Player">
                        </div>
                        <div class="card__content">
                            <h2 class="product__price"><%= coachh.age%></h2>
                            <p class="product__description"><%=coachh.description%></p>
                        </div>
                        <div class="card__actions">
                            <button class="btn">Add to Team</button>
                        </div>
                    </article>
                <% } %>
            </div>
        <% } else { %>
            <h1>No Products Found!</h1>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>